<section class="route info-page glossary">

  <header>
    <img src='/assets/images/logo.png' class="logo-image">
    <img src='/assets/images/logo_slocog.png' class="logo-image">

    {{link-to 'Back to CalBuilds' 'map'}}
  </header>

  <main>
    <h1>Glossary</h1>

    <ul class="terms-list">
      {{#each glossaryTerms as |term|}}
        {{glossary-list-item
          label=term.label
          definition=term.definition
          unitsLong=term.unitsLong
        }}
      {{/each}}
    </ul>
  </main>

  <footer>
    <img src="/assets/images/ambag-logo.png">
  </footer>

</section>
